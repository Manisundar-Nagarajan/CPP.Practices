#include<iostream>
using namespace std;
class A
{
        int x;
        int y;
        public:
        void setdata()
        {
                cout<<"Enter the data"<<endl;
                cin>>x>>y;
        }
        void print()
        {
                cout<<"x="<<x<<"y="<<y<<endl;
        }
        friend void add(A &,A &,A &);
};

void add(A &b1,A &b2,A &b3)
{
        b3.x=b1.x+b2.x;
        b3.y=b1.y+b2.y;
}
int main()
{
        A ob1,ob2,ob3;
        ob1.setdata();
        ob2.setdata();
        cout<<"Printing the obj1 values:"<<endl;
        ob1.print();
        cout<<"Printing the obj2 values:"<<endl;
        ob2.print();
        add(ob1, ob2, ob3);
        cout<<"Printing the obj3 values:"<<endl;
        ob3.print();
}
